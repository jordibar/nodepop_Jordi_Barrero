<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

    <% lista.forEach(function(anuncio) { %>
      <h2><%= anuncio.nombre %></h2>

      <img src="<%= anuncio.foto %>"></img>
      
      <% if (anuncio.venta) { %>
        <p>Venta</p>
      <% } else { %>
        <p>Busco</p>
      <% } %>

      <p><%= anuncio.precio %>â‚¬</p>

      <p><%= anuncio.tags %></p>
    
    
    <% }) %>




  </body>
</html>
